<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>

	<properties resource="conf.properties" />

	<!--定义iBATIS自身应用信息-->
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" maxRequests="200" maxSessions="100"
		maxTransactions="20" useStatementNamespaces="true" />

	<!-- page 公共提取-->
	<sqlMap resource="sqlmap/db2/ibatis-page.xml" />
    <!-- 系统管理相关-->
	<sqlMap resource="sqlmap/db2/sysutil/MenuInfo.xml" /> 
    <sqlMap resource="sqlmap/db2/common/sqlCom.xml" />
	<sqlMap resource="sqlmap/db2/sysutil/GZZCY.xml" />
	<sqlMap resource="sqlmap/db2/sysutil/XTYHJS.xml" />
	<sqlMap resource="sqlmap/db2/sysutil/XTYH.xml" />
</sqlMapConfig>