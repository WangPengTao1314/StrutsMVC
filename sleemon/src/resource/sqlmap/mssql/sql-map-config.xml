<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE sqlMapConfig      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>

	<properties resource="conf.properties" />

	<!--定义iBATIS自身应用信息-->
	<settings cacheModelsEnabled="true" enhancementEnabled="true"
		lazyLoadingEnabled="true" maxRequests="200" maxSessions="100"
		maxTransactions="20" useStatementNamespaces="true" />

	<!-- page 公共提取-->
	<sqlMap resource="sqlmap/mssql/ibatis-page.xml" />
	<!--例子XML-->
	<sqlMap resource="sqlmap/mssql/sample/newmasterslave/NewCpbltzd.xml" />
	<sqlMap resource="sqlmap/mssql/sample/newmasterslave/NewCpbltzdmx.xml" />
	<sqlMap resource="sqlmap/mssql/sample/newsingtable/newsingtable.xml" />
    <!-- 系统管理相关-->
    <sqlMap resource="sqlmap/mssql/system/CUST_QUERY.xml" />
	<sqlMap resource="sqlmap/mssql/system/sqlCom.xml" />
	<sqlMap resource="sqlmap/mssql/system/MenuInfo.xml" />
	<sqlMap resource="sqlmap/mssql/system/GZZCY.xml" />
	<sqlMap resource="sqlmap/mssql/system/XTYHJS.xml" />
	<sqlMap resource="sqlmap/mssql/system/XTYH.xml" />
	<sqlMap resource="sqlmap/mssql/system/XTJS.xml" />
	<sqlMap resource="sqlmap/mssql/system/GZZXX.xml" />
	<sqlMap resource="sqlmap/mssql/system/JGXX.xml" />
	<sqlMap resource="sqlmap/mssql/system/BMXX.xml" />
	<sqlMap resource="sqlmap/mssql/system/ZTXX.xml" />
	<sqlMap resource="sqlmap/mssql/system/RYXX.xml" />
	<sqlMap resource="sqlmap/mssql/system/YSGL.xml" />
	<sqlMap resource="sqlmap/mssql/system/FLOW.xml" />
	<sqlMap resource="sqlmap/mssql/system/SJZD.xml" />
	<sqlMap resource="sqlmap/mssql/system/SJZDMX.xml" /> 
    <sqlMap resource="sqlmap/mssql/system/XTSQ.xml" />
    <sqlMap resource="sqlmap/mssql/system/Numformat.xml" />
    <!-- 基础数据 规则维护-->
	<sqlMap resource="sqlmap/mssql/system/BMGZ.xml" />
	<sqlMap resource="sqlmap/mssql/system/BMGZMX.xml" />
    <!-- 编码规则共通 -->
	<sqlMap resource="sqlmap/mssql/system/sqlCommon.xml" />
    <!--首页-短消息-->
	<sqlMap resource="sqlmap/mssql/system/Message.xml" />
    <!-- 公告信息 -->
	<sqlMap resource="sqlmap/mssql/system/Notice.xml" />
</sqlMapConfig>